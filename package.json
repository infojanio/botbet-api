{
  "name": "boot-bet-backend",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "job:signals": "ts-node src/jobs/run-generate-signals.ts",
    "sync:data": "ts-node src/jobs/run-sync-external-data.ts",
    "generate:signals": "ts-node src/jobs/run-generate-signals.ts",
    "update:signals": "ts-node src/jobs/update-signal-results.ts",
    "update:history": "ts-node src/jobs/update-signal-history.ts",
    "sync:stats": "ts-node src/jobs/run-sync-stats.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "dayjs": "^1.11.18",
    "diacritics": "^1.3.0",
    "dotenv": "^17.2.3",
    "fastify": "^4.27.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/diacritics": "^1.3.3",
    "@types/node": "^20.14.9",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.13",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
